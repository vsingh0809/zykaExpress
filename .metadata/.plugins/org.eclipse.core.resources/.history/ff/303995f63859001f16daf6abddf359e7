package coding.strings;

public class ReverseString {

	public static void main(String[] args) {
	
		String ss="1234560";
		int length=ss.length();
		String s2="";
		for(int i=length-1;i>=0;i--) {
			s2=s2+ss.charAt(i);
		}
System.out.println(s2);
StringBuilder sb=new StringBuilder(ss);

	}
	
	
	
	

}
