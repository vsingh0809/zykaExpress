package coding.strings;

public class removeSpacialCharacter {

	public static void main(String[] args) {
		/*
		 * String s1="2233@@#$njbjh**^&#"; String s=s1.replaceAll("[^a-zA-Z0-9]", "");
		 * System.out.println(s);
		 */

	
	//------------------------------------------
	
	String ss="this isa col our full";
	String new1=ss.replace(" ", "1");
	System.out.println(new1);
	char[]ch=new1.toCharArray();
	int start=0;
	int end=ch.length-1;
	while(start<end) {
		char temp=ch[start];
		ch[start]=ch[end];
		ch[end]=temp;
		start++;
		end--;
		}
	
	System.out.println(ch);
		}
	
}
